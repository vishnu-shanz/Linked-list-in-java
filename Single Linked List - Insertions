import java.util.*;
    public class Main{
    public static void main(String[]args){
        Linkedlist link=new Linkedlist();
        link.insert(100);
        link.insert(200);
        link.insert(300);
        link.insertatbegin(500);
        link.display();
    }
}
class Node{
    int data;
    Node next;
    Node(int data){
        this.data=data;
        this.next=null;
    }
}
 class Linkedlist{
    Node head,tail;
    public void insert(int data){
        Node newNode=new Node(data);
        if(head==null){
            head=newNode;
            tail=newNode;
        }
        else{
            tail.next=newNode;
            tail=newNode;
        }
    }
    public void insertatbegin(int data){
          if(head==null){
            head=newNode;
            tail=newNode;
        }
    else{
        Node newNode=new Node(data);
        head=newNode;
        }
    }
    public void insertatspec(int pos,int data){
        Node newNode=new Node(data);
        Node temp=head;
        for(int i=0;i<pos-1;i++){
        temp=temp.next;
 }
        if(temp!=null){
            newNode.next=temp.next;
            temp.next=newNode;
        }
    }
    public void display(){
        Node temp=head;
        while(temp!=null){
            System.out.println(temp.data);
            temp=temp.next;
        }
    }
}
